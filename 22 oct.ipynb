   "source": [
    "## Identify duplicates in a list."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "541952e1-cb36-4427-96d7-15e6bb559f98",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[2, 4]\n"
     ]
    }
   ],
   "source": [
    "def find_duplicates(lst):\n",
    "    return [x for x in set(lst) if lst.count(x) > 1]\n",
    "\n",
    "print(find_duplicates([1, 2, 2, 3, 4, 4, 5]))  \n"
   ]
  },
